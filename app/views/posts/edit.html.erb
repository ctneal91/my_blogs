<h1>Posts#edit</h1>
<p>Find me in app/views/posts/edit.html.erb</p>

<%= form_for @post  do |f| %>


  <div>
    <%= f.label :title %>
    <%= f.text_field :title %><em><%= @post.errors[:title].join(", ") %></em>
  </div>

  <div>
    <%= f.label :body %>
    <%= f.text_field :body %><em><%= @post.errors[:body].join(", ") %></em>
  </div>



<% end %>
